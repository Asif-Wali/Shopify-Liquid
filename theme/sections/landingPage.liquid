<style>
  * {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
  }

  .loadingPage {
    width: 100%;
    height: auto;
    position: relative;
    z-index: 1000;
  }

  .loadingBg {
    width: 100%;
    height: auto;
    background-image: url('{{ 'Background Map.svg' | asset_url }}');
    background-size: cover;
    background-repeat: repeat;
    opacity: 1;
    transition: all 600ms ease-out;
  }

  .landingPage {
    display: none;
  }

  .logoBox {
    width: 356px;
    height: fit-content;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
    margin: -10px auto auto;
    transition: all 300ms ease-in-out;
    opacity: 1;
  }

  .logoImgSm {
    display: none
  }

  .logoBox > .logoImg {
    width: 75%;
    height: 240px;
    margin: auto;
  }

  .logoBox > .logoName {
    width: 100%;
    height: 90px;
  }

  .logoBox > .logoSlogan {
    width: 90%;
    height: 55px;
    text-align: center;
    font-family: "Amatic SC"
    , sans-serif;
    font-weight: 700;
    font-style: normal;
    font-size: 32px;
    color: #70645c;
    margin-top: 10px;
  }

  .iconBox {
    width: 77%;
    height: auto;
    margin: 40px auto auto;
    display: grid;
    grid-template-columns: repeat(5, 1fr);
    grid-template-rows: auto;
    justify-content: center;
    align-items: center;
    gap: 25px;
    padding-bottom: 20px;
    transition: all 300ms ease-out;
    opacity: 1;
  }

  .iconBox > div {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
  }

  .iconBox > div > img {
    width: 110px;
    height: 110px;
  }

  .PageDescription {
    width: 77%;
    height: auto;
    display: flex;
    flex-direction: column;
    gap: 15px;
    text-align: center;
    color: #70645c;
    margin: 25px auto auto;
  }

  .PageDescription > h1 {
    font-family: "Amatic SC"
    , sans-serif;
    font-weight: 700;
    font-style: normal;
    font-size: 50px;
  }

  .PageDescription > .DesclargeScreen {
    font-family: "Playfair Display"
    , serif;
    font-optical-sizing: auto;
    font-weight: 400;
    font-style: normal;
    font-size: 20px;
    letter-spacing: 1.2;
    word-spacing: 2px;
    line-height: 1.5;
  }

  .PageDescription > .DescsmallScreen {
    display: none;
  }

  .actionTabs {
    width: 77%;
    height: auto;
    margin: auto;
    display: flex;
    justify-content: space-evenly;
    align-items: center;
    padding: 20px 0;
    gap: 20px;
  }

  .readyMadeLgScrn,
  .customMadeLgScrn {
    width: 340px;
    height: 480px;
    display: flex;
    text-align: center;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    margin: auto;
    gap: 10px;
    border: 1px solid #707070;
    border-radius: 15px;
    padding-bottom: 20px;
  }

  .readyMadeSmScrn,
  .customMadeSmScrn {
    display: none;
  }

  .readyMadeLgScrn > div:nth-child(1),
  .customMadeLgScrn > div:nth-child(1) {
    width: 306px;
    height: 306px;
    text-align: center;
    position: relative;
  }

  .actionTabs > div > div > img {
    width: 100%;
    height: 100%;
  }

  .readyMadeLgScrn > div > .tabText {
    position: absolute;
    text-align: center;
    text-align: center;
    top: 34%;
    left: 34%;
  }

  .customMadeLgScrn > div > .tabText {
    position: absolute;
    text-align: center;
    text-align: center;
    top: 33%;
    left: 30%;
    margin: auto;
  }

  .tabText > h2 {
    width: 60%;
    color: white;
    font-family: "Amatic SC"
    , sans-serif;
    font-weight: 700;
    font-style: normal;
    font-size: 58px;
    line-height: 1;
    text-align: center;
  }

  .readyMadeLgScrn > p,
  .customMadeLgScrn > p {
    width: 95%;
    font-family: "Playfair Display"
    , serif;
    font-optical-sizing: auto;
    font-weight: 400;
    font-style: normal;
    font-size: 14px;
  }

  .actionTabBtn {
    width: 65%;
    height: 50px;
    background-color: white;
    border: 2px solid #707070;
    border-radius: 25px;
    box-shadow: rgba(0, 0, 0, 0.19) 0 10px 20px
    , rgba(0, 0, 0, 0.23) 0 6px 6px;
    margin-top: 5px;
  }

  .actionTabBtn > a {
    font-family: "Noto Sans"
    , sans-serif;
    font-weight: 300;
    font-style: normal;
    font-size: 32px;
    color: #70645c;
    text-decoration: none;
  }

  .readyMadeLgScrn:hover,
  .customMadeLgScrn:hover {
    background-color: #ede9ed;
    border: 3px solid #707070;
  }

  @media all and (max-width: 768px) {
    .iconBox {
      width: 85%;
      height: auto;
      display: grid;
      grid-template-columns: repeat(4, 1fr);
    }

    .iconBox > div:nth-child(9) {
      display: none;
    }

    .iconBox > div:nth-child(10) {
      display: none;
    }

    .iconBox > div > img {
      width: 100px;
      height: 100px;
    }
  }

  @media all and (max-width: 599px) {
    .iconBox {
      width: 95%;
      height: auto;
      grid-template-columns: repeat(3, 1fr);
      padding-bottom: 30px;
    }

    .iconBox > div:nth-child(10) {
      display: none;
    }

    .iconBox > div > img {
      width: 83px;
      height: 83px;
    }

    .logoBox {
      max-width: 356px;
      width: 100%;
      height: fit-content;
    }

    .logoImgSm {
      display: block;
      width: 100%;
      height: auto;
    }

    .logoBox > .logoImg {
      display: none
    }

    .logoBox > .logoName {
      display: none;
    }

    .logoBox > .logoSlogan {
      width: 90%;
      height: 50px;
      margin: auto;
    }

    .landingContent {
      width: 100%;
      height: 620px;
    }

    .landingContent > .PageDescription {
      width: 100%;
      height: auto;
      display: flex;
      flex-direction: column;
      gap: 5px;
      text-align: center;
      color: #70645c;
      margin: 0 auto auto;
      padding: 39px 0;
    }

    .landingContent > .PageDescription > h1 {
      font-family: "Amatic SC"
      , sans-serif;
      font-weight: 700;
      font-style: normal;
      font-size: 32px;
    }

    .landingContent > .PageDescription > .DesclargeScreen {
      display: none;
    }

    .landingContent > .PageDescription > .DescsmallScreen {
      width: 70%;
      display: block;
      font-family: "Playfair Display"
      , serif;
      font-weight: 400;
      font-style: normal;
      font-size: 18px;
      margin: auto;
      line-height: 1.2;
    }

    .landingContent > .actionTabs {
      width: 100%;
      height: auto;
      margin: auto;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      gap: 0;
      padding: 0;
    }

    .readyMadeLgScrn,
    .customMadeLgScrn {
      display: none;
    }

    .readyMadeSmScrn,
    .customMadeSmScrn {
      display: block;
      width: 100%;
      height: auto;
      display: flex;
      justify-content: center;
      align-items: center;
      margin: auto;
      gap: 10px;
      padding: 10px 15px;
    }

    .readyMadeSmScrn {
      background-color: #e8e8e8;
    }

    .customMadeSmScrn {
      background-color: white;
    }

    .readyMadeSmScrn > div:nth-child(1),
    .customMadeSmScrn > div:nth-child(2) {
      width: 65%;
      height: 137px;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      text-align: center;
      gap: 20px;
    }

    .actionTabBtn {
      width: 40%;
      height: 31px;
      font-size: 18px;
    }

    .actionTabBtn > a {
      font-family: "Noto Sans"
      , sans-serif;
      font-weight: 300;
      font-style: normal;
      font-size: 18px;
      color: #70645c;
      text-decoration: none;
    }

    .readyMadeSmScrn > div > p,
    .customMadeSmScrn > div > p {
      height: 67px;
      width: 95%;
      font-family: "Playfair Display"
      , serif;
      font-optical-sizing: auto;
      font-weight: 400;
      font-style: normal;
      font-size: 14px;
      line-height: 1.2;
    }

    .readyMadeSmScrn > div:nth-child(2),
    .customMadeSmScrn > div:nth-child(1) {
      width: 35%;
      height: 180px;
      text-align: center;
      margin: auto;
      position: relative;
    }

    .readyMadeSmScrn > div:nth-child(2) > img,
    .customMadeSmScrn > div:nth-child(1) > img {
      width: 100%;
      height: 100%;
      margin: auto;
    }

    .readyMadeSmScrn > div > .tabTextSm {
      position: absolute;
      text-align: center;
      text-align: center;
      top: 38%;
      left: 26%;
    }

    .customMadeSmScrn > div > .tabTextSm {
      position: absolute;
      text-align: center;
      text-align: center;
      top: 36%;
      left: 24%;
      margin: auto;
    }

    .tabTextSm > h2 {
